import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    String s[]=sc.nextLine().split(" ");
	    int x=s.length;
	    int z=0;
	    for(int i=0;i<x;i++){
	        z=z+Integer.parseInt(s[i]);
	    }
	    System.out.print(z);
	}
}
