import java.util.*;
public class Main{
public static void main(String[] args){   
    Scanner aaaa=new Scanner(System.in); int n=1245568;
    String s=Integer.toString(n);
    int x=s.length();
    char arr[]=s.toCharArray();
        HashSet<Character> sc=new HashSet<>();

    for(int i=0;i<x;i++){
      sc.add(arr[i]);
